<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
	
	<bean id="setterMethod" class="com.kh.myapp.member.vo.MemberVO">
		<property name="id"  value="admin@kh.com"/>
		<property name="passwd" value="1234"/>
	</bean>
	
	<bean id="Contructor" class="com.kh.myapp.member.vo.MemberVO">
		<constructor-arg ref="setterMethod" />
	</bean>
	
	
</beans>